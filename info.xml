<?xml version="1.0"?>
<extension key="org.civicrm.l10nx" type="module">
  <file>l10nx</file>
  <name>l10n Extension</name>
  <description>Provides an interface for l10n related tools and extensions</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>CiviCRM LLC</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/bjendres/org.civicrm.l10nx</url>
    <url desc="Documentation">https://github.com/bjendres/org.civicrm.l10nx</url>
    <url desc="Support">https://github.com/bjendres/org.civicrm.l10nx/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>0.1-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>Conceived and kicked off at the Bamford sprint 2018</comments>
  <civix>
    <namespace>CRM/L10nx</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_L10nx_Upgrader</upgrader>
</extension>
